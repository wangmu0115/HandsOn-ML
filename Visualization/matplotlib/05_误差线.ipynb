   "source": [
    "x = np.linspace(0, 10, 100)\n",
    "y = np.sin(x)\n",
    "\n",
    "plt.errorbar(x, y, yerr=1.2, fmt=\".r\", ecolor=\"b\");"
   ]
  },
  {
